<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml" alignment="CENTER" spacing="20.0"
      fx:controller="com.example.client.view.AppViewModel">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <Label text="DreamCars"/>
    <Button text="%weather" onAction="#onWeatherButtonClick"/>
    <Button text="%cars" onAction="#onCarsButtonClick"/>
    <Button text="%engines" onAction="#onEnginesButtonClick"/>
    <Button text="%equipment" onAction="#onEquipmentOptionsButtonClick"/>
    <Button text="%settings" onAction="#onSettingsButtonClick"/>
    <Button text="%logout" onAction="#onLogoutButtonClick"/>
</VBox>
